	.

fibo-ind-ei-ei:hasIndicatorValue
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
	rdfs:label "has indicator value" ;
	rdfs:comment "QName: fibo-ind-ei-ei:hasIndicatorValue" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/IND/EconomicIndicators/EconomicIndicators/> ;
	rdfs:range fibo-fnd-qt-qtu:QuantityValue ;
