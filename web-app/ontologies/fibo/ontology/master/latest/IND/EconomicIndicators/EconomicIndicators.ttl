fibo-ind-ei-ei:AverageDailyEarnings
	a owl:Class ;
	rdfs:subClassOf
		fibo-ind-ei-ei:AverageEarnings ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-utl-alx:hasReferencePeriod ;
			owl:hasValue fibo-ind-ei-ei:Daily ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-ind-ei-ei:hasIndicatorValue ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-fnd-utl-alx:ArithmeticMean
					fibo-fnd-acc-cur:MonetaryAmount
				) ;
			] ;
		]
		;
	rdfs:label "average daily earnings" ;
	rdfs:comment "QName: fibo-ind-ei-ei:AverageDailyEarnings" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/IND/EconomicIndicators/EconomicIndicators/> ;
	skos:definition "a measure of the average daily wage an employee makes over the reporting period" ;
	fibo-fnd-utl-av:adaptedFrom "http://stats.oecd.org/glossary/detail.asp?ID=4360"^^xsd:anyURI ;
	.

fibo-ind-ei-ei:AverageEarnings
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-utl-alx:Expression ,
		fibo-ind-ei-ei:EconomicIndicator ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-utl-alx:hasReferencePeriod ;
			owl:onClass fibo-fnd-dt-fd:ExplicitRecurrenceInterval ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "average earnings" ;
	rdfs:comment "QName: fibo-ind-ei-ei:AverageEarnings" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/IND/EconomicIndicators/EconomicIndicators/> ;
	skos:definition "a measure of the average wage an hourly or salaried worker makes in a given period of time" ;
	fibo-fnd-utl-av:adaptedFrom "http://stats.oecd.org/glossary/detail.asp?ID=4360"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "Average earnings are typically calculated on an hourly, daily, weekly, or monthly basis. They may be expressed as an amount of money or in terms of a percent change with respect to a prior period, depending on the jurisdiction and report." ;
	.

