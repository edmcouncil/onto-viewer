fibo-fbc-pas-fpas:AgencyAgreement
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-agr-ctr:MutualContractualAgreement ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-agr-ctr:hasContractParty ;
			owl:someValuesFrom fibo-fbc-pas-fpas:RegisteredAgent ;
		]
		;
	rdfs:label "agency agreement" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:AgencyAgreement" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	rdfs:seeAlso
		<http://www.sos.state.tx.us/corp/registeredagents.shtml> ,
		<https://thelawdictionary.org/agent/>
		;
	skos:definition "an agreement that designates a party as a registered agent to represent and act on behalf of another party in some, typically legal, financial, or medical capacity" ;
	.

fibo-fbc-pas-fpas:AgentForServiceOfProcess
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-pas-fpas:RegisteredAgent ;
	rdfs:label "agent for service of process" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:AgentForServiceOfProcess" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	rdfs:seeAlso
		<http://www.law.cornell.edu/wex/agent_for_service_of_process> ,
		<http://www.sos.state.tx.us/corp/registeredagents.shtml>
		;
	skos:definition "a registered agent (person or organization) designated by a business entity, such as a corporation, to receive legal correspondence on behalf of the business entity in the jurisdiction in which the agent's address is located" ;
	fibo-fnd-utl-av:explanatoryNote "The person may be an officer of the corporation or a third party, such as the corporation's attorney, or a company providing such agency services." ;
	.

fibo-fbc-pas-fpas:AmendedTrade
	a
		owl:NamedIndividual ,
		fibo-fbc-pas-fpas:TradeLifecycleStage
		;
	rdfs:label "amended trade" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:AmendedTrade" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "individual representing an amended trade state" ;
	.

fibo-fbc-pas-fpas:Basket
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-arr:Collection ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-arr-arr:hasConstituent ;
			owl:someValuesFrom fibo-fbc-pas-fpas:BasketConstituent ;
		]
		;
	rdfs:label "basket" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:Basket" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "collection of goods, services, or other things (e.g., financial contracts) that can be purchased and sold in some marketplace" ;
	fibo-fnd-utl-av:adaptedFrom
		"http://www.investopedia.com/terms/m/market_basket.asp"^^xsd:anyURI ,
		"http://www.merriam-webster.com/dictionary/market%20basket"^^xsd:anyURI
		;
	fibo-fnd-utl-av:explanatoryNote
		"A basket may be associated with a specific market sector, and may be delineated for the purposes of statistical analysis, such as for calculating CPI. According to the US Bureau of Labor Statistics (BLS), with respect to the CPI, a market basket is a package of goods and services that consumers purchase for day-to-day living. The weight of each item is based on the amount of expenditure reported by a sample of households." ,
		"From a securities perspective, a basket is a collection of products or securities that are designated to mimic the performance of a market. For investors, the market basket is the principal idea behind index funds, which are essentially a broad sample of stocks, bonds or other securities in the market; this provides investors with a benchmark against which to compare their investment returns."
		;
	.

fibo-fbc-pas-fpas:BasketConstituent
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-arr-arr:CollectionConstituent ;
	rdfs:label "basket constituent" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:BasketConstituent" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	owl:equivalentClass [
		a owl:Restriction ;
		owl:onProperty fibo-fnd-arr-arr:isConstituentOf ;
		owl:someValuesFrom fibo-fbc-pas-fpas:Basket ;
	] ;
	skos:definition "component of a basket" ;
	.

fibo-fbc-pas-fpas:Broker
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-agr-ctr:ContractThirdParty ,
		[
			a owl:Class ;
			owl:unionOf (
				fibo-fbc-pas-fpas:LicensedAgent
				fibo-fbc-pas-fpas:ThirdPartyAgent
			) ;
		]
		;
	rdfs:label "broker" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:Broker" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "any party that acts as an intermediary between a buyer and a seller, usually charging a commission" ;
	fibo-fnd-utl-av:adaptedFrom
		"17 CFR 45.1, Definitions - see the definition of agent" ,
		"Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014"
		;
	fibo-fnd-utl-av:explanatoryNote "A broker that specializes in stocks, bonds, commodities, or certain derivatives must be registered with the exchange in which the securities are traded." ;
	.

fibo-fbc-pas-fpas:BrokerDealer
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-fct-fct:FunctionalEntity ,
		[
			a owl:Class ;
			owl:unionOf (
				fibo-fbc-pas-fpas:Broker
				fibo-fbc-pas-fpas:Dealer
			) ;
		]
		;
	rdfs:label "broker-dealer" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:BrokerDealer" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "any party in the business of buying and selling securities, operating as both a broker and a dealer, depending on the transaction" ;
	fibo-fnd-utl-av:adaptedFrom
		"Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ,
		"Office of Financial Research (OFR) Annual Report, 2012, Glossary"
		;
	.

fibo-fbc-pas-fpas:Catalog
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-fct-pub:Publication ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-aap-agt:isIdentifiedBy ;
			owl:allValuesFrom fibo-fnd-arr-id:Identifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:characterizes ;
			owl:someValuesFrom fibo-fnd-pas-pas:Product ;
		]
		;
	rdfs:label "catalog" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:Catalog" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "a list of goods and/or services available for sale with their description and possibly prices, published as a printed or  electronic document (e-catalog)" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/catalog.html"^^xsd:anyURI ;
	.

fibo-fbc-pas-fpas:ClearedTrade
	a
		owl:NamedIndividual ,
		fibo-fbc-pas-fpas:TradeLifecycleStage
		;
	rdfs:label "cleared trade" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:ClearedTrade" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "individual representing a cleared trade state" ;
	.

fibo-fbc-pas-fpas:ClosedTrade
	a
		owl:NamedIndividual ,
		fibo-fbc-pas-fpas:TradeLifecycleStage
		;
	rdfs:label "closed trade" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:ClosedTrade" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "individual representing a closed trade state" ;
	.

fibo-fbc-pas-fpas:Dealer
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-pas-pas:ServiceProvider ,
		[
			a owl:Class ;
			owl:unionOf (
				fibo-fnd-pas-pas:Buyer
				fibo-fnd-pas-pas:Seller
			) ;
		]
		;
	rdfs:label "dealer" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:Dealer" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "any party that purchases goods or services for resale and acts on their own behalf in a transaction" ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
	fibo-fnd-utl-av:explanatoryNote "A dealer is a counterparty or principal in the transaction with the customer." ;
	.

fibo-fbc-pas-fpas:Exposure
	a owl:Class ;
	rdfs:label "exposure" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:Exposure" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "the extent to which an individual or organization is unprotected and open to damage, danger, risk of suffering a loss, or uncertainty" ;
	skos:example "Examples include financial exposure, credit exposure, legal exposure, credit rating exposure, reputational exposure, and so forth." ;
	fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/exposure.html"^^xsd:anyURI ;
	.

fibo-fbc-pas-fpas:FinancialExposure
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-pas-fpas:Exposure ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:appliesTo ;
			owl:someValuesFrom fibo-fnd-pty-pty:PartyInRole ;
		]
		;
	rdfs:label "financial exposure" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:FinancialExposure" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "the extent to which an individual or organization is open to risk of suffering a loss in a transaction, or with respect to some investment or set of investments, e.g., some holding; the amount one stands to lose in that transaction or investment" ;
	skos:example "Examples in banking include the total amount of unsecured loans, the amount of loans advanced to a single borrower, group, industry, or country, and the probability of loss from devaluation, revaluation, or foreign exchange fluctuations." ;
	fibo-fnd-utl-av:adaptedFrom
		"Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ,
		"http://www.businessdictionary.com/definition/exposure.html"^^xsd:anyURI
		;
	.

fibo-fbc-pas-fpas:FinancialIntermediationService
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-pas-fpas:FinancialService ;
	rdfs:label "financial intermediation service" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:FinancialIntermediationService" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "any financial service in which a third party (the intermediary) matches lenders and investors with entrepreneurs and other borrowers in need of capital" ;
	fibo-fnd-utl-av:definitionOrigin "Office of Financial Research (OFR) Annual Report, 2012, Glossary" ;
	fibo-fnd-utl-av:explanatoryNote "Often investors and borrowers do not have precisely matching needs, and the intermediary's capital is put at risk to transform the credit risk and maturity of the liabilities to meet the needs of investors." ;
	.

fibo-fbc-pas-fpas:FinancialProduct
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-pas-pas:Product ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
			owl:someValuesFrom fibo-fbc-pas-fpas:FinancialServiceProvider ;
		]
		;
	rdfs:label "financial product" ;
	rdfs:comment "QName: fibo-fbc-pas-fpas:FinancialProduct" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/> ;
	skos:definition "a product provided to consumers and businesses by financial institutions such as banks, insurance companies, brokerage firms, consumer finance companies, and investment companies all of which comprise the financial services industry" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.investorwords.com/19080/financial_services.html"^^xsd:anyURI ;
	.

fibo-fbc-pas-fpas:FinancialProductCatalog
