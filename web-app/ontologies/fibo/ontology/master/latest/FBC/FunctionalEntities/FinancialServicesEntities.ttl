	rdfs:subPropertyOf fibo-fnd-dt-fd:hasDate ;
	rdfs:label "has date insured" ;
	rdfs:comment "QName: fibo-fbc-fct-fse:hasDateInsured" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/FinancialServicesEntities/> ;
	rdfs:range fibo-fnd-dt-fd:Date ;
