@prefix : <https://spec.sae.com/sae/ontology/foundations/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sae-fnd: <https://spec.sae.com/sae/ontology/foundations/> .
@base <https://spec.sae.com/sae/ontology/foundations/> .

<https://spec.sae.com/sae/ontology/foundations/> rdf:type owl:Ontology ;
                                                  owl:imports <https://www.omg.org/spec/Commons/20220901/PartiesAndSituations/> ,
                                                              <https://www.omg.org/spec/Commons/Documents/> ,
                                                              <https://www.omg.org/spec/Commons/QuantitiesAndUnits/> ,
                                                              <https://www.omg.org/spec/Commons/RolesAndCompositions/> .

#################################################################
#    Annotation properties
#################################################################

###  https://spec.sae.com/sae/ontology/foundations/hasDiscoveryMethod
sae-fnd:hasDiscoveryMethod rdf:type owl:AnnotationProperty ;
                           rdfs:label "has discovery method"@en ;
                           rdfs:range sae-fnd:DiscoveryMethod .


#################################################################
#    Object Properties
#################################################################

###  https://spec.sae.com/sae/ontology/foundations/hasObject
sae-fnd:hasObject rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <https://www.omg.org/spec/Commons/Collections/comprises> ;
                  owl:inverseOf sae-fnd:isObjectOf ;
                  rdfs:label "has object"@en .


###  https://spec.sae.com/sae/ontology/foundations/hasQuantityKind
sae-fnd:hasQuantityKind rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <https://www.omg.org/spec/Commons/Collections/comprises> ;
                        rdfs:label "has quantity kind"@en .


###  https://spec.sae.com/sae/ontology/foundations/hasQuantityValue
sae-fnd:hasQuantityValue rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <https://www.omg.org/spec/Commons/Collections/comprises> ;
                         rdfs:label "has quantity value"@en .


###  https://spec.sae.com/sae/ontology/foundations/isObjectOf
sae-fnd:isObjectOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <https://www.omg.org/spec/Commons/Collections/isIncludedIn> ;
                   rdfs:label "is object of"@en .


#################################################################
#    Data properties
#################################################################

###  https://spec.sae.com/sae/ontology/foundations/hasPercentage
sae-fnd:hasPercentage rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf <https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasNumericValue> ;
                      rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:onDatatype xsd:decimal ;
                                   owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                          ]
                                                          [ xsd:maxInclusive 100.0
                                                          ]
                                                        )
                                 ] ;
                      rdfs:label "has percentage"@en .


###  https://spec.sae.com/sae/ontology/foundations/hasPercentageInComposition
sae-fnd:hasPercentageInComposition rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf sae-fnd:hasPercentage ;
                                   rdf:type owl:FunctionalProperty ;
                                   rdfs:domain <https://www.omg.org/spec/Commons/Collections/Constituent> ;
                                   rdfs:label "has percentage in composition"@en .


#################################################################
#    Classes
#################################################################

###  https://spec.sae.com/sae/ontology/foundations/DiscoveryMethod
sae-fnd:DiscoveryMethod rdf:type owl:Class ;
                        rdfs:subClassOf <https://www.omg.org/spec/Commons/Documents/Specification> ;
                        rdfs:label "discovery method"@en .


###  https://spec.sae.com/sae/ontology/foundations/MaterialComposition
sae-fnd:MaterialComposition rdf:type owl:Class ;
                            rdfs:subClassOf <https://www.omg.org/spec/Commons/RolesAndCompositions/Composition> ;
                            rdfs:label "material composition"@en .


###  https://spec.sae.com/sae/ontology/foundations/MaterialConstituent
sae-fnd:MaterialConstituent rdf:type owl:Class ;
                            rdfs:subClassOf <https://www.omg.org/spec/Commons/Collections/Constituent> ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty sae-fnd:hasPercentageInComposition ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onDataRange xsd:decimal
                                            ] ;
                            rdfs:label "material constituent"@en .


###  https://spec.sae.com/sae/ontology/foundations/ObjectHasProperty
sae-fnd:ObjectHasProperty rdf:type owl:Class ;
                          rdfs:subClassOf <https://www.omg.org/spec/Commons/PartiesAndSituations/Situation> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty sae-fnd:hasObject ;
                                            owl:cardinality "1"^^xsd:nonNegativeInteger
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty sae-fnd:hasQuantityKind ;
                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onClass <https://www.omg.org/spec/Commons/QuantitiesAndUnits/QuantityKind>
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty sae-fnd:hasQuantityValue ;
                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onClass <https://www.omg.org/spec/Commons/QuantitiesAndUnits/ScalarQuantityValue>
                                          ] ;
                          rdfs:label "object has property"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> "that an object has a certain property"@en .


###  https://spec.sae.com/sae/ontology/foundations/SpectrochemicalMethod
sae-fnd:SpectrochemicalMethod rdf:type owl:Class ;
                              rdfs:subClassOf sae-fnd:DiscoveryMethod ;
                              rdfs:label "spectrochemical method"@en .


###  https://spec.sae.com/sae/ontology/foundations/Standard
sae-fnd:Standard rdf:type owl:Class ;
                 rdfs:subClassOf <https://www.omg.org/spec/Commons/Documents/ReferenceDocument> ;
                 rdfs:label "standard" .


